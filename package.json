{
    "name": "cadence-discord-bot",
    "version": "6.0.0",
    "description": "A free Discord music bot. No locked functionality, no premium tier, no ads.",
    "repository": {
        "type": "git",
        "url": "https://github.com/mariusbegby/cadence-discord-bot"
    },
    "bugs": {
        "url": "https://github.com/mariusbegby/cadence-discord-bot/issues"
    },
    "homepage": "https://github.com/mariusbegby/cadence-discord-bot#readme",
    "author": {
        "name": "Marius Begby",
        "url": "https://github.com/mariusbegby"
    },
    "publisher": "mariusbegby",
    "license": "MIT",
    "main": "dist/index.js",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": ["**/__tests__/**/*.(spec|test).ts"],
        "testPathIgnorePatterns": ["/node_modules/", "src/__tests__/_mocks/"],
        "moduleNameMapper": {
            "^@core/(.*)$": "<rootDir>/src/core/$1",
            "^@events/(.*)$": "<rootDir>/src/events/$1",
            "^@interactions/(.*)$": "<rootDir>/src/interactions/$1",
            "^@services/(.*)$": "<rootDir>/src/services/$1",
            "^@utilities/(.*)$": "<rootDir>/src/utilities/$1",
            "^@validation/(.*)$": "<rootDir>/src/validation/$1",
            "^@config/(.*)$": "<rootDir>/config/$1",
            "^@type/(.*)$": [
                "<rootDir>/src/core/_types/$1",
                "<rootDir>/src/events/_types/$1",
                "<rootDir>/src/interactions/_types/$1",
                "<rootDir>/src/services/_types/$1",
                "<rootDir>/src/utilities/_types/$1",
                "<rootDir>/src/validation/_types/$1"
            ],
            "^@mocks/(.*)$": "<rootDir>/src/__tests__/_mocks/$1"
        }
    },
    "scripts": {
        "postinstall": "pnpm dlx prisma generate",
        "prebuild": "pnpm toc",
        "toc": "i18next-resources-for-ts interface -i ./locales/en -o ./locales/resources.d.ts",
        "build": "tsc && tsc-alias -p tsconfig.json",
        "start": "node .",
        "test": "jest",
        "coverage": "jest --coverage",
        "lint": "biome lint --write",
        "format": "biome format --write",
        "restart": "pnpm build && pnpm start"
    },
    "dependencies": {
        "@discord-player/extractor": "^4.5.0",
        "@prisma/client": "^5.17.0",
        "config": "^3.3.12",
        "discord-player": "^6.7.0",
        "discord-player-youtubei": "^1.2.0",
        "discord-voip": "^0.1.3",
        "dotenv": "^16.4.5",
        "eris": "^0.17.2",
        "i18next": "^23.12.2",
        "i18next-fs-backend": "^2.3.1",
        "mediaplex": "^0.0.9",
        "pino": "^9.3.2",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@biomejs/biome": "1.8.3",
        "@types/config": "^3.3.4",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.12",
        "i18next-resources-for-ts": "^1.5.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.3",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.5.4"
    },
    "optionalDependencies": {
        "pino-loki": "^2.3.0",
        "pino-pretty": "^11.2.2"
    }
}
